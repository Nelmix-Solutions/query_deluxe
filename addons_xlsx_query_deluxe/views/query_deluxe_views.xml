<?xml version="1.0"?>

<odoo>
	<record id="query_deluxe_view_form" model="ir.ui.view">
		<field name="name">Xlsx addons</field>
		<field name="model">querydeluxe</field>
		<field name="inherit_id" ref="query_deluxe.query_deluxe_view_form"/>
		<field name="arch" type="xml">
			<xpath expr="//button[@name='print_result']" position="after">
				<button name="print_xlsx" string="Print XLSX" type="object" class="oe_highlight" attrs="{'invisible':[('raw_output','in',['',[],False])]}"/>
			</xpath>
		</field>
	</record>
</odoo>
